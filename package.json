{
  "name": "@front-utils/linter",
  "version": "0.3.0",
  "description": "Configuration files for linting",
  "author": "IVGolubev",
  "license": "ISC",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/front-util/linter.git"
  },
  "exports": {
    ".": "./src/index.js"
  },
  "scripts": {
    "check-lint": "eslint . --max-warnings=0 --fix",
    "publish:linter": "npm publish --access public"
